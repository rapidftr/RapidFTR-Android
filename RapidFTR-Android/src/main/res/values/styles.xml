<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <color name="links">#0087ef</color>
    <color name="logoBackground">#31353f</color>
    <style name="AppTheme" parent="android:Theme.Holo.Light"/>

    <style name="page_title">
        <item name="android:background">#aaaaaa</item>
        <item name="android:textColor">#333333</item>
        <item name="android:padding">3dp</item>
    </style>

    <style name="save_btn">
        <item name="android:drawableLeft">@drawable/icon_save</item>
        <item name="android:textColor">#333333</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="Theme.RapidFTRtheme" parent="AppTheme">
        <item name="android:spinnerStyle">@style/Spinner</item>
        <item name="android:spinnerItemStyle">@style/SpinnerItem</item>
        <item name="android:spinnerDropDownItemStyle">@style/DropDownItem</item>
        <item name="android:editTextStyle">@style/TextView</item>
    </style>

    <style name="Spinner" parent="@android:style/Widget.Spinner">
        <item name="android:background">@drawable/spinner</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_margin">0dp</item>
    </style>

    <style name="SpinnerItem" parent="@android:style/Widget.TextView.SpinnerItem">
        <item name="android:textColor">#333333</item>
        <item name="android:padding">3dp</item>

    </style>

    <style name="DropDownItem" parent="@android:style/Widget.DropDownItem">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="TextView" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/textbox</item>
        <item name="android:textColor">#333333</item>
        <item name="android:typeface">sans</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="button">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:textSize">20dp</item>
    </style>

    <style name="button_photo_audio">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>

        <item name="android:layout_margin">5dp</item>
        <item name="android:textSize">18dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">32dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>

    <style name="save_button">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textSize">16dp</item>
        <item name="android:drawableLeft">@drawable/icon_save</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="links">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="formpanel">
        <item name="android:padding">5dp</item>
        <item name="android:background">@drawable/bg_android</item>
    </style>

    <style name="tab">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:padding">5dp</item>

    </style>

    <style name="tab_sel">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="childrow">
        <item name="android:padding">2dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="childid">
        <item name="android:color">#666666</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="childname">
        <item name="android:color">#333333</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingBottom">2dp</item>
    </style>

    <style name="photoThumbnail">
        <item name="android:padding">1dp</item>
    </style>

    <style name="no_child_text">
        <item name="android:color">#333333</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">italic|bold</item>

    </style>
</resources>